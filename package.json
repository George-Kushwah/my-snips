{
  "name": "sab-kuch",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "rsbuild dev",
    "build": "rsbuild build",
    "preview": "rsbuild preview",
    "test": "jest --watchAll=false --coverage --testResultsProcessor jest-sonar-reporter -u",
    "sonar": "node sonarqube-scanner.js",
    "server": "tsx watch server/server.ts",
    "dev1": "concurrently \"npm run dev\" \"npm run server\""
  },
  "dependencies": {
    "@babel/preset-react": "^7.26.3",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@popperjs/core": "^2.11.8",
    "@reduxjs/toolkit": "^2.5.0",
    "@rsbuild/plugin-sass": "^1.1.1",
    "@tanstack/react-query": "^5.66.9",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.14",
    "@types/react-redux": "^7.1.34",
    "@types/redux-saga": "^0.9.31",
    "axios": "^1.7.9",
    "babel-jest": "^29.7.0",
    "body-parser": "^1.20.3",
    "bootstrap": "^5.3.3",
    "concurrently": "^9.1.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-sonar-reporter": "^2.0.0",
    "mysql": "^2.18.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-error-boundary": "^5.0.0",
    "react-paginate": "^8.3.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.28.0",
    "redux-saga": "^1.3.0",
    "sass": "^1.82.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.1.4",
    "@rsbuild/plugin-react": "^1.0.7",
    "@types/express": "^5.0.0",
    "@types/mysql": "^2.15.26",
    "@types/node": "^22.10.2",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.2",
    "sonarqube-scanner": "^4.2.6",
    "typescript": "^5.7.2"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverage": false,
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "resetMocks": true,
    "collectCoverageFrom": [
      "src/layout/*.{js,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/__test__/*.tsx",
      "!src/app.tsx",
      "!src/index.tsx"
    ],
    "coveragePathIgnorePatterns": [
      "src/Redux/*.ts",
      "src/layout/Axios-list.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 80
      }
    }
  }
}
